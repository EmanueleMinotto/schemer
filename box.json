{
    "alias": "schemer.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": [
        "src"
    ],
    "extract": false,
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests",
                "tests"
            ],
            "in": "vendor"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "bin/schemer",
    "output": "schemer-@git-version@.phar",
    "stub": true,
    "web": false
}
