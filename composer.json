{
    "name": "emanueleminotto/schemer",
    "description": "json-schema.org based validation and formatting",
    "keywords": [
        "json",
        "schema",
        "utility"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuele Minotto",
            "email": "minottoemanuele@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~5.2",
        "justinrainbow/json-schema": "~1.3",
        "kherge/box": "^2.5",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\Schemer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\Schemer\\Test\\": "tests/"
        }
    },
    "bin": [
        "bin/schemer"
    ]
}
